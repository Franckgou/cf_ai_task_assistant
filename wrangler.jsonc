{
  "$schema": "./node_modules/wrangler/config-schema.json",
  "name": "cf-ai-task-assistant",
  "main": "src/index.js",
  "compatibility_date": "2024-10-01",
  "ai": {
    "binding": "AI"
  },
  "durable_objects": {
    "bindings": [
      {
        "name": "TASK_AGENT",
        "class_name": "TaskAgent"
      }
    ]
  },
  "migrations": [
    {
      "tag": "v1",
      "new_sqlite_classes": [
        "TaskAgent"
      ]
    }
  ]
}
